{
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "Console": {
    "LogLevel": {
      "Default": "None"
    }
  },
  "api_gateway": {
    "endpoint": "http://localhost:8080",
    "routing": {
      "postCoin": "coin",
      "getCoin": "coin/{0}",
      "getMessage": "message/{0}",
      "postMessage": "message",
      "getMessages": "messages/{0}",
      "getMessageRange": "messages/{0}/{1}/{2}",
      "getMessageCount": "message/count/{0}"
    }
  },
  "network": {
    "environment": "testnet",
    "enabled_restAPI": 0,
    "restApi_endpoint": "http://localhost:8001/"
  }
}
